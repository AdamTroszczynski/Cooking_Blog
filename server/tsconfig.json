{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es5",
    "module": "CommonJS",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "paths": {
      "@/*": ["./src/*"]
    },
  }
}
